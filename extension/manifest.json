{
  "manifest_version": 2,
  
  "key": "",

  "applications": {
    "gecko": {
      "id": "gpsio@caltopo.com",
      "strict_min_version": "50.0"
    }
  },
  
  "name": "gpsio",
  "description": "GPS IO",
  "version": "0.0.7",
  
  "browser_action": {
    "default_icon": "gps.png",
    "default_popup": "popup.html"
   },

  "permissions": [
    "nativeMessaging","storage"
    ],
  
  "background": {
    "scripts": ["background.js"]
  },
    
  "content_scripts": [{"matches": ["https://*/*","http://*/*"], "js": ["content_script.js"]}]
}